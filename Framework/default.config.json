{
  "configVersion": 1,
  "repo": {
    "id": "repo.local.default",
    "channel": "stable"
  },
  "logging": {
    "level": "info",
    "retentionDays": 14,
    "maxFileMB": 10
  },
  "safety": {
    "mode": "safe",
    "nonInteractive": {
      "onPolicyPrompt": "deny"
    }
  },
  "cache": {
    "downloads": {
      "ttlDays": 30,
      "maxGB": 5
    },
    "repos": {
      "ttlDays": 7,
      "maxGB": 2
    }
  },
  "concurrency": {
    "maxParallelDownloads": 4,
    "maxRunspaces": 4
  },
  "telemetry": {
    "enabled": false,
    "localOnly": true
  },
  "localization": {
    "locale": "en-US"
  },
  "frameworkVersion": "1.0.4"
}